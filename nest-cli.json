{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/craft-project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/craft-project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/craft-project",
  "projects": {
    "craft-project": {
      "type": "application",
      "root": "apps/craft-project",
      "entryFile": "main",
      "sourceRoot": "apps/craft-project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/craft-project/tsconfig.app.json"
      }
    },
    "task": {
      "type": "application",
      "root": "apps/task",
      "entryFile": "main",
      "sourceRoot": "apps/task/src",
      "compilerOptions": {
        "tsConfigPath": "apps/task/tsconfig.app.json"
      }
    },
    "libs": {
      "type": "library",
      "root": "libs/libs",
      "entryFile": "index",
      "sourceRoot": "libs/libs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/libs/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "photo": {
      "type": "application",
      "root": "apps/photo",
      "entryFile": "main",
      "sourceRoot": "apps/photo/src",
      "compilerOptions": {
        "tsConfigPath": "apps/photo/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "context": {
      "type": "library",
      "root": "libs/context",
      "entryFile": "index",
      "sourceRoot": "libs/context/src",
      "compilerOptions": {
        "tsConfigPath": "libs/context/tsconfig.lib.json"
      }
    },
    "caching": {
      "type": "library",
      "root": "libs/caching",
      "entryFile": "index",
      "sourceRoot": "libs/caching/src",
      "compilerOptions": {
        "tsConfigPath": "libs/caching/tsconfig.lib.json"
      }
    }
  }
}